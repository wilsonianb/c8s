    "@hapi/h2o2": "^8.3.2",
    "@hapi/hapi": "^18.4.0",
    "@hapi/inert": "^5.2.1",
    "@hapi/joi": "^14.5.0",
    "@hapi/vision": "^5.5.3",
    "axios": "^0.19.0",
    "canonical-json": "0.0.4",
    "debug": "^3.1.0",
    "enjoi": "^6.0.0",
    "formik": "^1.5.8",
    "handlebars": "^4.3.1",
    "kubernetes-client": "^8.3.4",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-web-monetization": "0.0.6",
    "reduct": "^3.2.0",
    "riverpig": "^1.1.3",
    "thirty-two": "^1.0.2",
    "through2": "^3.0.1"
  },
  "devDependencies": {
    "@types/debug": "0.0.30",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/hapi__boom": "^7.4.1",
    "@types/hapi__h2o2": "^8.3.0",
    "@types/hapi__inert": "^5.2.0",
    "@types/hapi__vision": "^5.5.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/through2": "^2.0.33",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "jest": "^24.9.0",
    "jest-enzyme": "^7.1.1",
    "json-schema-to-typescript": "^5.5.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^6.0.2",
    "tslint": "^5.20.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.6.3",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8"
  }
