{
  "name": "codiusd",
  "version": "1.2.5",
  "description": "Codius Host Software",
  "homepage": "https://github.com/codius/codiusd#readme",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "files": [
    "src/**/*.ts",
    "src/**/*.js",
    "src/**/*.js.map",
    "src/**/*.html",
    "src/schemas/*.json",
    "src/util/*.json"
  ],
  "scripts": {
    "build": "npm run schema-to-tsd && npm run compile-ts && webpack",
    "schema-to-tsd": "node scripts/build-schema-types",
    "compile-ts": "tsc --project .",
    "lint": "tslint --project .",
    "test": "npm run lint && jest",
    "watch": "mocha-typescript-watch",
    "prepare": "npm run build",
    "start": "node src/index.js"
  },
  "bin": "./src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codius/codiusd.git"
  },
  "keywords": [
    "hosting",
    "smart-contract",
    "vm",
    "container",
    "pod",
    "host",
    "server"
  ],
  "author": "Codius Team <info@codius.org>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/codius/codiusd/issues"
  },
